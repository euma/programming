/*直接在别人的代码基础上修改有好处，也有坏处，可是我觉得最重要的还是自己的掌控！*/
#include<cstdio>
const int MAX_N = 50;
int n, a[MAX_N];
void
solve ()
{
  int ThePerimeterOfTheLargest = (-1);
  for (int i = 0; i < n; i++)
    {
      for (int j = 0; j < n; j++)
	{
	  for (int k = 0; k < n; k++)
	    {
	      int ThePerimeter;
	      if ((a[i] + a[j] > a[j]) || (a[i] + a[j] > a[j])
		  || (a[i] + a[j] > a[j]))
		{
		  ThePerimeter = a[i] + a[j] + a[k];
		}
	      else
		{
		  ThePerimeter = (-1);
		}

	      if (ThePerimeter > ThePerimeterOfTheLargest)
		{
		  ThePerimeterOfTheLargest = ThePerimeter;
		}
	    }
	}
    }
}

if (f)
  puts ("Yes");
else
  puts ("No");
}

int
main ()
{
//从标准输入读入
  scanf ("%d %d", &n, &m);
  for (int i = 0; i < n; i++)
    {
      scanf ("%d", &k[i]);
    }
  solve ();

  return 0;
}
