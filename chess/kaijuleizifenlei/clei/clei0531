[Event "Baku FIDE GP"]
[Site "Baku"]
[Date "2008.04.21"]
[Round "1"]
[White "Cheparinov, Ivan"]
[Black "Grischuk, Alexander"]
[Result "0-1"]
[ECO "C18"]
[WhiteElo "2696"]
[BlackElo "2716"]
[Annotator "Postny,E"]
[PlyCount "84"]
[EventDate "2008.04.21"]
