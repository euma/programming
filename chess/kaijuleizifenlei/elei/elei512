[Event "World op 36th"]
[Site "Philadelphia"]
[Date "2008.07.06"]
[Round "8"]
[White "Mikhalevski, Victor"]
[Black "Smirin, Ilia"]
[Result "1/2-1/2"]
[ECO "E97"]
[WhiteElo "2592"]
[BlackElo "2637"]
[Annotator "Mikhalevski,V"]
[PlyCount "129"]
[EventDate "2008.07.02"]
[EventType "swiss"]
[EventRounds "9"]
