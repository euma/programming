[Event "EU-Cup 24th"]
[Site "Kallithea"]
[Date "2008.10.21"]
[Round "5"]
[White "Sasikiran, Krishnan"]
[Black "Radjabov, Teimour"]
[Result "0-1"]
[ECO "A65"]
[WhiteElo "2694"]
[BlackElo "2751"]
[Annotator "Radjabov"]
[PlyCount "102"]
