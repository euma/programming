[Event "Corus-C"]
[Site "Wijk aan Zee"]
[Date "2008.01.22"]
[Round "9"]
[White "Caruana, Fabiano"]
[Black "Van der Wiel, John TH"]
[Result "1-0"]
[ECO "B66"]
[WhiteElo "2598"]
[BlackElo "2490"]
[Annotator "Caruana"]
[PlyCount "87"]
[EventDate "2008.01.12"]
[EventType "tourn"]
[EventRounds "13"]
[EventCountry "NED"]
[EventCategory "10"]
[Source "ChessBase"]
