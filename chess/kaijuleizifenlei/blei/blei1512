[Round "5"]
[White "Berg, Emanuel"]
[Black "Carlsson, Pontus"]
[Result "1-0"]
[ECO "B23"]
[WhiteElo "2601"]
[BlackElo "2514"]
[Annotator "Berg,E"]
[PlyCount "67"]
